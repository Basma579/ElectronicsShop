@model IEnumerable<ElectronicsShop.ViewModels.CategoryViewModel>

@{
    ViewData["Title"] = "ListCategory";
}

<br>
<h1>List Category</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
      
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
         
        </tr>
}
    </tbody>
</table>
